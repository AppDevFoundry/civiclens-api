						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code is 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Response has bills array\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.bills).to.be.an('array');",
									"    pm.expect(jsonData.bills.length).to.be.greaterThan(0);",
									"});",
									"",
									"pm.test(\"Response has pagination\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.pagination).to.exist;",
									"    pm.expect(jsonData.pagination.count).to.be.a('number');",
									"});",
									"",
									"pm.test(\"Bill has required fields\", function () {",
									"    var jsonData = pm.response.json();",
									"    var bill = jsonData.bills[0];",
									"    pm.expect(bill.congress).to.be.a('number');",
									"    pm.expect(bill.type).to.be.a('string');",
									"    pm.expect(bill.number).to.exist;",
									"    pm.expect(bill.url).to.be.a('string');",
									"});"
								],
								"type": "text/javascript"
							}
						}
