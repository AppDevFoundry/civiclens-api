{
  "name": "@api/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "test:api": "nx test --testPathPattern=integration",
    "test:unit": "nx test --testPathPattern='^((?!integration).)*$'",
    "test:watch": "nx test --watch",
    "test:coverage": "nx test --coverage",
    "sync:bills": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/sync-congress.ts bills",
    "sync:members": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/sync-congress.ts members",
    "sync:hearings": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/sync-congress.ts hearings",
    "sync:all": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/sync-congress.ts all",
    "sync:status": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/sync-congress.ts status",
    "enrich:bills": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/enrich-bills.ts",
    "dev:token": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/generate-test-token.ts",
    "dev:test-api": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/test-api-endpoints.ts",
    "test:notifications": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/test-notifications.ts",
    "notify:process": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/process-notifications.ts",
    "notify:digest:daily": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/process-notifications.ts --digest",
    "notify:digest:weekly": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/process-notifications.ts --digest --weekly",
    "test:parallel": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/test-parallel-sync.ts",
    "test:parallel-fetch": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/test-parallel-fetch.ts",
    "test:config": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/test-performance-config.ts",
    "profile": "ts-node --transpile-only --compiler-options '{\"module\":\"CommonJS\"}' src/scripts/profile-performance.ts"
  },
  "private": true,
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} src/prisma/seed.ts",
    "schema": "src/prisma/schema.prisma"
  },
  "dependencies": {
    "@ngneat/falso": "^7.1.1",
    "@prisma/client": "^6.19.0",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "~4.18.1",
    "express-jwt": "^8.4.1",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^7.0.10",
    "pg-boss": "^12.2.0",
    "slugify": "^1.6.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nx/esbuild": "17.2.6",
    "@nx/eslint": "17.2.6",
    "@nx/eslint-plugin": "17.2.6",
    "@nx/jest": "17.2.6",
    "@nx/js": "17.2.6",
    "@nx/node": "17.2.6",
    "@nx/workspace": "17.2.6",
    "@swc-node/register": "~1.6.7",
    "@swc/core": "~1.3.85",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "~4.17.13",
    "@types/jest": "^29.4.0",
    "@types/nock": "^10.0.3",
    "@types/node": "18.16.9",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "esbuild": "^0.19.2",
    "eslint": "~8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "jest-mock-extended": "^3.0.5",
    "nock": "^14.0.10",
    "nx": "17.2.6",
    "prettier": "^2.6.2",
    "prisma": "^6.19.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.2.2"
  }
}
